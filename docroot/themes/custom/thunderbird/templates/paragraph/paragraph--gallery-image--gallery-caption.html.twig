{#
/**
 * @file
 * Theme implementation to display the Gallery Caption view mode of a gallery
 * image paragraph.
 */
#}
{%
  set classes = [
    'caption'
  ]
%}
{% block paragraph %}
  <div{{ attributes.addClass(classes) }}>
    {% block content %}
      {{ content }}
    {% endblock %}
  </div>
{% endblock paragraph %}