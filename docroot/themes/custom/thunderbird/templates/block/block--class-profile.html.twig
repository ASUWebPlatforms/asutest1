{% set classes = [ 
    'class-profile',  
    'block', 
    'block-' ~ configuration.provider|clean_class, 
    'block-' ~ plugin_id|clean_class
  ]
%}
<div {{ attributes.addClass(classes) }}>
  {{ title_prefix }}
  {{ title_suffix }}
  <div class="container">
    <div class="row">
      {{ content }}
    </div>
  </div>
</div>